{
  "name": "fetchtastic",
  "version": "0.3.4",
  "description": "Predictable and strongly typed fetch requests.",
  "homepage": "https://fetchtastic-docs.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/fveracoechea/fetchtastic"
  },
  "bugs": {
    "url": "https://github.com/fveracoechea/fetchtastic/issues"
  },
  "author": "Francisco Veracoechea",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsup --watch",
    "type-check": "tsc --project tsconfig.json",
    "build": "tsup",
    "test": "jest --ci --maxWorkers=2",
    "lint": "eslint --max-warnings 0",
    "format": "prettier --write ."
  },
  "packageManager": "pnpm@9.0.6",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8",
    "eslint-config": "*",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "fetch",
    "ajax",
    "request",
    "type-safe"
  ]
}
