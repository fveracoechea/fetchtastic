{
  "name": "fetchtastic",
  "version": "0.1.5",
  "description": "Predictable and strongly typed fetch requests.",
  "homepage": "https://fetchtastic-docs.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/fveracoechea/fetchtastic"
  },
  "bugs": {
    "url": "https://github.com/fveracoechea/fetchtastic/issues"
  },
  "author": "Francisco Veracoechea",
  "license": "MIT",
  "main": "./dist/cjs/index.cjs",
  "typings": "./dist/types/index.d.ts",
  "module": "./dist/esm/index.js",
  "type": "module",
  "sideEffects": false,
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/*/index.d.ts",
        "./dist/types/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js"
    },
    "./core": {
      "types": "./dist/types/core/index.d.ts",
      "import": "./dist/esm/core/index.js",
      "require": "./dist/cjs/core/index.cjs"
    },
    "./utils": {
      "types": "./dist/types/utils/index.d.ts",
      "import": "./dist/esm/utils/index.js",
      "require": "./dist/cjs/utils/index.cjs"
    },
    "./react": {
      "types": "./dist/types/react/index.d.ts",
      "import": "./dist/esm/react/index.js",
      "require": "./dist/cjs/react/index.cjs"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "rimraf dist && rollup -c",
    "test": "jest --ci --maxWorkers=2",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "format": "prettier --write .",
    "http-codegen": "ts-node scripts/generateHttpCodes"
  },
  "packageManager": "yarn@3.5.0",
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-multi-entry": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.9",
    "eslint": "^8.36.0",
    "eslint-config": "*",
    "jest": "^29.5.0",
    "prettier": "2.8.7",
    "rimraf": "^4.4.1",
    "rollup": "^3.20.2",
    "rollup-plugin-progress": "^1.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig": "*",
    "tslib": "^2.5.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "fetch",
    "ajax",
    "request",
    "type-safe"
  ]
}
