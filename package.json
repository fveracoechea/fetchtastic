{
  "name": "fetchtastic-monorepo",
  "version": "0.0.0",
  "private": true,
  "size-limit": [
    {
      "name": "CommonJS",
      "path": "lib/dist/index.cjs",
      "limit": "6 KB"
    },
    {
      "name": "ES Modules",
      "path": "lib/dist/index.js",
      "limit": "6 KB"
    }
  ],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "size": "size-limit --json",
    "prettier-check": "prettier '**/*.{js,ts,jsx,tsx,json,md,mdx}' --check",
    "prettier-format": "prettier --write \"**/*.{js,ts,jsx,tsx,json,md,mdx}\""
  },
  "volta": {
    "node": "20.12.2",
    "pnpm": "9.0.6"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "prettier": "^3.2.5",
    "size-limit": "^11.1.2",
    "turbo": "^1.13.3",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=6"
  },
  "packageManager": "pnpm@9.0.6"
}
